source ~/ros2_ws/install/setup.bash
ros2 launch the_construct_office_gazebo warehouse_ur3e.launch.xml

ros2 control list_controllers
ros2 topic echo /joint_states --once

colcon build --packages-select my_moveit_config
cd ~/ros2_ws/ && colcon build --packages-select moveit2_scripts
cd ~/ros2_ws/ && colcon build --packages-select my_moveit_config
cd ~/ros2_ws/ && colcon build --packages-select real_moveit_config
cd ~/ros2_ws/ && colcon build --packages-select sim_ur3e_config real_ur3e_config ur3e_helper_scripts

                S I M
source ~/ros2_ws/install/setup.bash && ros2 launch my_moveit_config move_group.launch.py
source ~/ros2_ws/install/setup.bash && ros2 launch my_moveit_config moveit_rviz.launch.py
source ~/ros2_ws/install/setup.bash && ros2 launch moveit2_scripts pick_and_place.launch.py
                R E A L
source ~/ros2_ws/install/setup.bash && ros2 launch real_moveit_config move_group.launch.py
source ~/ros2_ws/install/setup.bash && ros2 launch real_moveit_config moveit_rviz.launch.py
source ~/ros2_ws/install/setup.bash && ros2 launch moveit2_scripts pick_and_place_real.launch.py


