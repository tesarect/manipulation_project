ros2 launch real_moveit_config moveit_rviz.launch.py

source ~/ros2_ws/install/setup.bash && ros2 launch moveit2_scripts pick_and_place_perception.launch.py
source ~/ros2_ws/install/setup.bash && ros2 launch moveit2_scripts pick_and_place_perception_real.launch.py
source ~/ros2_ws/install/setup.bash && ros2 launch moveit2_scripts pick_and_place_perception.launch.py > output.log 2>&1   

cd ~/ros2_ws && colcon build --packages-select moveit2_scripts && source install/setup.bash
cd ~/ros2_ws && colcon build --packages-select object_detection && source install/setup.bash
cd ~/ros2_ws && colcon build --packages-select object_detection my_moveit_config && source install/setup.bash
cd ~/ros2_ws && colcon build --packages-select moveit2_scripts object_detection my_moveit_config && source install/setup.bash

ros2 run rqt_console rqt_console
ros2 run tf2_tools view_frames

ros2 bag record /object_detected /surface_detected /object_markers /bench_markers -o sim_block
ros2 bag record /object_detected /surface_detected /object_markers /bench_markers -o real_block

ros2 bag play sim_block --loop
ros2 bag play real_block --loop

cd ~/ros2_ws/src/zenoh-pointcloud/
./install_zenoh.sh
cd ~/ros2_ws/src/zenoh-pointcloud/init/
./rosject.sh

