source ~/ros2_ws/install/setup.bash && ros2 launch real_moveit_config move_group.launch.py

source ~/ros2_ws/install/setup.bash && ros2 launch object_detection object_detection.launch.py
source ~/ros2_ws/install/setup.bash && ros2 launch object_detection object_detection_real.launch.py

